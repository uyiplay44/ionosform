{
  "builds": [
    { "src": "/static/**", "use": "@now/static" },
    { "src": "/app.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "/app.py" }
  ],
  "env": {
    "PYTHONUNBUFFERED": "true",
    "FLASK_ENV": "production",
    "FLASK_APP": "app.py"
  }
}
